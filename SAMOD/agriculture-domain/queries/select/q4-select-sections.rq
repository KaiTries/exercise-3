PREFIX was: <https://was-course.interactions.ics.unisg.ch/farm-ontology#>

SELECT ?sections WHERE {
GRAPH <https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-kai#> {
   bind (<https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-kai#farm> as ?farm)
   ?farm a was:Farm.
   ?farm was:hasSection ?sections.
   ?sections a was:Section.
 }
}
