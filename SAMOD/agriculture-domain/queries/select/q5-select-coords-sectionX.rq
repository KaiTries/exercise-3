PREFIX was: <https://was-course.interactions.ics.unisg.ch/farm-ontology#>

SELECT ?x1 ?y1 ?x2 ?y2 WHERE {
GRAPH <https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-kai#> {
   bind (<https://sandbox-graphdb.interactions.ics.unisg.ch/was-exercise-3-kai#section1> as ?section)
   ?section a was:Section.
   ?section was:isAt ?coordinates.
   ?coordinates was:x1 ?x1.
   ?coordinates was:y1 ?y1.
   ?coordinates was:x2 ?x2.
   ?coordinates was:y2 ?y2.
 }
}